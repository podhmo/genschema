{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "sqlc-conf",
  "description": "Generated by `./bin/genschema --schema-title sqlc-conf --indent    --query github.com/sqlc-dev/sqlc/internal/config.Config`",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "version": {
      "type": "string"
    },
    "project": {
      "$ref": "#/$defs/Project"
    },
    "cloud": {
      "$ref": "#/$defs/Cloud"
    },
    "sql": {
      "type": "array",
      "items": {
        "$ref": "#/$defs/SQL"
      }
    },
    "overrides": {
      "$ref": "#/$defs/Gen"
    },
    "plugins": {
      "type": "array",
      "items": {
        "$ref": "#/$defs/Plugin"
      }
    },
    "rules": {
      "type": "array",
      "items": {
        "$ref": "#/$defs/Rule"
      }
    }
  },
  "required": [
    "version",
    "project",
    "cloud",
    "sql",
    "plugins",
    "rules"
  ],
  "$defs": {
    "Cloud": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "organization": {
          "type": "string"
        },
        "project": {
          "type": "string"
        },
        "hostname": {
          "type": "string"
        }
      },
      "required": [
        "organization",
        "project",
        "hostname"
      ]
    },
    "Codegen": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "out": {
          "type": "string"
        },
        "plugin": {
          "type": "string"
        },
        "options": {
          "type": "go:invalid type"
        }
      },
      "required": [
        "out",
        "plugin",
        "options"
      ],
      "description": "TODO: Figure out a better name for this"
    },
    "Database": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "uri": {
          "type": "string"
        }
      },
      "required": [
        "uri"
      ]
    },
    "Engine": {
      "type": "string"
    },
    "Gen": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "go": {
          "type": "object",
          "additionalProperties": false,
          "items": {
            "$ref": "#/$defs/GenGo"
          }
        }
      }
    },
    "GenGo": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "overrides": {
          "type": "array",
          "items": {
            "$ref": "#/$defs/Override"
          }
        },
        "rename": {
          "type": "object",
          "additionalProperties": {
            "type": "string"
          }
        }
      }
    },
    "GoStructTag": {
      "type": "string"
    },
    "GoType": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "import": {
          "type": "string"
        },
        "package": {
          "type": "string"
        },
        "type": {
          "type": "string"
        },
        "pointer": {
          "type": "boolean"
        },
        "slice": {
          "type": "boolean"
        },
        "Spec": {
          "type": "string"
        },
        "BuiltIn": {
          "type": "boolean"
        }
      },
      "required": [
        "import",
        "package",
        "type",
        "pointer",
        "slice",
        "Spec",
        "BuiltIn"
      ]
    },
    "Match": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "Regexp": {
          "type": "object",
          "additionalProperties": false,
          "items": {
            "$ref": "#/$defs/Regexp"
          }
        }
      }
    },
    "Override": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "go_type": {
          "$ref": "#/$defs/GoType"
        },
        "go_struct_tag": {
          "$ref": "#/$defs/GoStructTag"
        },
        "db_type": {
          "type": "string"
        },
        "postgres_type": {
          "type": "string"
        },
        "engine": {
          "$ref": "#/$defs/Engine"
        },
        "nullable": {
          "type": "boolean"
        },
        "unsigned": {
          "type": "boolean"
        },
        "null": {
          "type": "boolean"
        },
        "column": {
          "type": "string"
        },
        "ColumnName": {
          "type": "object",
          "additionalProperties": false,
          "items": {
            "$ref": "#/$defs/Match"
          }
        },
        "TableCatalog": {
          "type": "object",
          "additionalProperties": false,
          "items": {
            "$ref": "#/$defs/Match"
          }
        },
        "TableSchema": {
          "type": "object",
          "additionalProperties": false,
          "items": {
            "$ref": "#/$defs/Match"
          }
        },
        "TableRel": {
          "type": "object",
          "additionalProperties": false,
          "items": {
            "$ref": "#/$defs/Match"
          }
        },
        "GoImportPath": {
          "type": "string"
        },
        "GoPackage": {
          "type": "string"
        },
        "GoTypeName": {
          "type": "string"
        },
        "GoBasicType": {
          "type": "boolean"
        },
        "GoStructTags": {
          "type": "object",
          "additionalProperties": {
            "type": "string"
          }
        }
      },
      "required": [
        "go_type",
        "go_struct_tag",
        "db_type",
        "postgres_type",
        "nullable",
        "unsigned",
        "null",
        "column",
        "GoImportPath",
        "GoPackage",
        "GoTypeName",
        "GoBasicType",
        "GoStructTags"
      ]
    },
    "Paths": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "Plugin": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "name": {
          "type": "string"
        },
        "process": {
          "type": "object",
          "additionalProperties": false,
          "items": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
              "cmd": {
                "type": "string"
              }
            },
            "required": [
              "cmd"
            ]
          }
        },
        "wasm": {
          "type": "object",
          "additionalProperties": false,
          "items": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
              "url": {
                "type": "string"
              },
              "sha256": {
                "type": "string"
              }
            },
            "required": [
              "url",
              "sha256"
            ]
          }
        }
      },
      "required": [
        "name"
      ]
    },
    "Project": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "id": {
          "type": "string"
        }
      },
      "required": [
        "id"
      ]
    },
    "Regexp": {
      "type": "object",
      "additionalProperties": false,
      "properties": {}
    },
    "Rule": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "name": {
          "type": "string"
        },
        "rule": {
          "type": "string"
        },
        "message": {
          "type": "string"
        }
      },
      "required": [
        "name",
        "rule",
        "message"
      ]
    },
    "SQL": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "engine": {
          "$ref": "#/$defs/Engine"
        },
        "schema": {
          "$ref": "#/$defs/Paths"
        },
        "queries": {
          "$ref": "#/$defs/Paths"
        },
        "database": {
          "type": "object",
          "additionalProperties": false,
          "items": {
            "$ref": "#/$defs/Database"
          }
        },
        "strict_function_checks": {
          "type": "boolean"
        },
        "strict_order_by": {
          "type": "boolean",
          "items": {
            "type": "boolean"
          }
        },
        "gen": {
          "$ref": "#/$defs/SQLGen"
        },
        "codegen": {
          "type": "array",
          "items": {
            "$ref": "#/$defs/Codegen"
          }
        },
        "rules": {
          "type": "array",
          "items": {
            "type": "string"
          }
        }
      },
      "required": [
        "schema",
        "queries",
        "strict_function_checks",
        "gen",
        "codegen",
        "rules"
      ]
    },
    "SQLGen": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "go": {
          "type": "object",
          "additionalProperties": false,
          "items": {
            "$ref": "#/$defs/SQLGo"
          }
        },
        "json": {
          "type": "object",
          "additionalProperties": false,
          "items": {
            "$ref": "#/$defs/SQLJSON"
          }
        }
      }
    },
    "SQLGo": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "emit_interface": {
          "type": "boolean"
        },
        "emit_json_tags": {
          "type": "boolean"
        },
        "json_tags_id_uppercase": {
          "type": "boolean"
        },
        "emit_db_tags": {
          "type": "boolean"
        },
        "emit_prepared_queries": {
          "type": "boolean"
        },
        "emit_exact_table_names": {
          "type": "boolean"
        },
        "emit_empty_slices": {
          "type": "boolean"
        },
        "emit_exported_queries": {
          "type": "boolean"
        },
        "emit_result_struct_pointers": {
          "type": "boolean"
        },
        "emit_params_struct_pointers": {
          "type": "boolean"
        },
        "emit_methods_with_db_argument": {
          "type": "boolean"
        },
        "emit_pointers_for_null_types": {
          "type": "boolean"
        },
        "emit_enum_valid_method": {
          "type": "boolean"
        },
        "emit_all_enum_values": {
          "type": "boolean"
        },
        "json_tags_case_style": {
          "type": "string"
        },
        "package": {
          "type": "string"
        },
        "out": {
          "type": "string"
        },
        "overrides": {
          "type": "array",
          "items": {
            "$ref": "#/$defs/Override"
          }
        },
        "rename": {
          "type": "object",
          "additionalProperties": {
            "type": "string"
          }
        },
        "sql_package": {
          "type": "string"
        },
        "sql_driver": {
          "type": "string"
        },
        "output_batch_file_name": {
          "type": "string"
        },
        "output_db_file_name": {
          "type": "string"
        },
        "output_models_file_name": {
          "type": "string"
        },
        "output_querier_file_name": {
          "type": "string"
        },
        "output_files_suffix": {
          "type": "string"
        },
        "inflection_exclude_table_names": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "query_parameter_limit": {
          "type": "integer",
          "items": {
            "type": "integer"
          }
        },
        "omit_unused_structs": {
          "type": "boolean"
        }
      },
      "required": [
        "emit_interface",
        "emit_json_tags",
        "json_tags_id_uppercase",
        "emit_db_tags",
        "emit_prepared_queries",
        "emit_exported_queries",
        "emit_result_struct_pointers",
        "emit_params_struct_pointers",
        "emit_pointers_for_null_types",
        "package",
        "out",
        "sql_package",
        "sql_driver"
      ]
    },
    "SQLJSON": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "out": {
          "type": "string"
        },
        "indent": {
          "type": "string"
        },
        "filename": {
          "type": "string"
        }
      },
      "required": [
        "out"
      ]
    }
  }
}
